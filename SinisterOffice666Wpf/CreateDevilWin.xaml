<Window x:Class="SinisterOffice666Wpf.CreateDevilWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SinisterOffice666Wpf"
        mc:Ignorable="d"
        Title="CreateDevilWin" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Ник"/>
            <TextBox Text="{Binding Nick}"/>
            <TextBlock Text="Ранг"/>
            <TextBox Text="{Binding Rank}"/>
            <TextBlock Text="Год"/>
            <TextBox Text="{Binding Year}"/>
            <Button Content="Добавить" Width="150" Margin="30" />
        </StackPanel>
    </Grid>
</Window>
